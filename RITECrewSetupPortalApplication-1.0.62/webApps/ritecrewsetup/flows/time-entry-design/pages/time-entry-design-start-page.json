{
  "title": "time entry design",
  "description": "",
 "variables": {
  "dynamicColumns": {
    "type": "DynamicColumn[]",
    "defaultValue": [
      { "key": "A", "header": "A" },
      { "key": "B", "header": "B" }
    ]
  },
  "newCombinationRow": {
    "type": "any",
    "defaultValue": {}
  },
  "combinationDataList": {
    "type": "any[]",
    "defaultValue": [
      { 
        "id": 1,
        "label": "Project",
        "A": "",
        "B": "",
        "total": 0,
        "isNewRow": false
      },
      { 
        "id": 2,
        "label": "Task",
        "A": "",
        "B": "",
        "total": 0,
        "isNewRow": false
      },
      { 
        "id": 3,
        "label": "Shift",
        "A": "",
        "B": "",
        "total": 0,
        "isNewRow": false
      },
      { 
        "id": 4,
        "label": "Work Package",
        "A": "",
        "B": "",
        "total": 0,
        "isNewRow": false
      },
      { 
        "id": 5,
        "label": "Location",
        "A": "",
        "B": "",
        "total": 0,
        "isNewRow": false
      }
    ]
  },
  "combinationADP": {
    "type": "vb/ArrayDataProvider2",
    "defaultValue": {
      "data": "{{ $variables.combinationDataList }}",
      "keyAttributes": "id"
    }
  },
  "employeeDataList": {
    "type": "any[]",
    "defaultValue": [
      {
        "id": 1,
        "employee": "E1234 John",
        "timeType": "REG",
        "jobCode": "LABOR",
        "A": 0,
        "B": 0,
        "total": 0
      }
    ]
  },
  "employeeADP": {
    "type": "vb/ArrayDataProvider2",
    "defaultValue": {
      "data": "{{ $variables.employeeDataList }}",
      "keyAttributes": "id"
    }
  },
  "combinationColumns": {
    "type": "any[]",
    "defaultValue": []
  },
  "employeeColumns": {
    "type": "any[]",
    "defaultValue": []
  },
  "illustrationBackground": {
    "type": "string",
    "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-bg-01.png"
  },
  "illustrationForeground": {
    "type": "string",
    "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-fg-01.png"
  }
},
  "metadata": {},
 "types": {
  "DynamicColumn": {
    "key": "string",
    "header": "string"
  }
},
  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chain": "addColumnActionChain",
          "parameters": {
            "event": "{{$event}}",
            "originalEvent": "{{$event.detail.originalEvent}}"
          }
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-bind-dom": {
        "path": "ojs/ojbinddom"
      },
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-collapsible": {
        "path": "ojs/ojcollapsible"
      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-number": {
        "path": "ojs/ojinputnumber"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-menu": {
        "path": "ojs/ojmenu"
      },
      "oj-menu-button": {
        "path": "ojs/ojbutton"
      },
      "oj-option": {
        "path": "ojs/ojoption"
      },
      "oj-select-single": {
        "path": "ojs/ojselectsingle"
      },
      "oj-sp-welcome-page": {
        "path": "oj-sp/welcome-page/loader"
      },
      "oj-table": {
        "path": "ojs/ojtable"
      }

   }
  }
