{
  "title": "crew time entry management",
  "description": "",
  "variables": {
    "columns": {
      "type": "object[]",
      "defaultValue": "[[$functions.getTableColumns()]]"
    },
    "crew": {
      "type": "string"
    },
    "crewDate": {
      "type": "any"
    },
    "CrewNameLOVSDP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "itemType": "getCrewRite_CrewDetailsSearch",
        "keyAttributes": "crewsetup_id"
      }
    },
    "crewsetup_id": {
      "type": "string"
    },
    "crewStatusADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "itemType": "status",
        "keyAttributes": "crew_details"
      }
    },
    "dateRangeADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "itemType": "dateRangeType",
        "keyAttributes": "range"
      }
    },
    "detailsADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "itemType": "timeEntryObj",
        "keyAttributes": "employee_number"
      }
    },
    "illustrationBackground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-bg-01.png "
    },
    "illustrationForeground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-fg-01.png"
    },
    "imageStretch": {
      "type": "string",
      "defaultValue": "none"
    },
    "mobileImage": {
      "type": "string",
      "defaultValue": "on"
    },
    "timeEntryADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "itemType": "getCrewRite_MoniteringSearch2",
        "keyAttributes": "crewsetup_id"
      }
    },
    "timeEntryObj": {
      "type": {
        "crew_name": "string",
        "crew_setupid": "string",
        "day": "string",
        "status": "string",
        "week": "string"

      }
    },
    "weekADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {

        "itemType": "weekObj",
        "keyAttributes": "label"
      }
    },
    "weekArray": {
      "type": "object[]",
      "defaultValue": [
        {
          "id": 1,
          "value": "Monday",
          "label": "01-12-2025"
        },
        {
          "id": 2,
          "value": "Tuesday",
          "label": "02-12-2025"
        },
        {
          "id": 3,
          "value": "Wednesday",
          "label": "03-12-2025"
        },
        {
          "id": 4,
          "value": "Thursday",
          "label": "04-12-2025"
        },
        {
          "id": 5,
          "value": "Friday",
          "label": "05-12-2025"
        },
        {
          "id": 6,
          "value": "Saturday",
          "label": "06-12-2025"
        },
        {
          "id": 7,
          "value": "Sunday",
          "label": "07-12-2025"
        }
      ]
    }

  },
  "metadata": {},
  "types": {
    "@decorators.getCrewRite_CrewDetailsSearch": {
      "endpoint": "getCrewRite_CrewDetailsSearch",
      "service": "CrewRite_ORDS",
      "path": "response-_-items[0]"
    },
    "@decorators.getCrewRite_MoniteringSearch2": {
      "endpoint": "getCrewRite_MoniteringSearch2",
      "service": "CrewRite_ORDS",
      "path": "response-_-items[0]"
    },
    "dateRangeType": {
      "range": "string"
    },
    "getCrewRite_CrewDetailsSearch": {
      "crew_name": "string",
      "crewsetup_id": "number"
    },
    "getCrewRite_MoniteringSearch2": {
      "crew_name": "string",
      "crew_week": "string",
      "crewsetup_id": "number",
      "friday": "string",
      "monday": "string",
      "saturday": "string",
      "sunday": "string",
      "supervisor": "string",
      "supervisor_id": "string",
      "thursday": "string",
      "tuesday": "string",
      "wednesday": "string"

    },
    "status": {
      "crew_details": "string"
    },
    "tableObj": {
      "crew_count": "string",
      "crew_name": "string",
      "friday": "string",
      "id": "number",
      "monday": "string",
      "saturday": "string",
      "sunday": "string",
      "supervisor": "string",
      "thursday": "string",
      "tuesday": "string",
      "wednesday": "string"
    },
    "timeEntryObj": {
      "employee_number": "string",
      "resource_name": "string",
      "status": "string",
      "total_hours": "string"
    },
    "weekObj": {
      "id": "number",
      "label": "string",
      "value": "string"
    }

  },
  "eventListeners": {
    "closeAction": {
      "chains": [
        {
          "chain": "ButtonActionChain",
          "parameters": {
            "event": "{{$event}}",
            "originalEvent": "{{$event.detail.originalEvent}}"
          }
        }
      ]
    },
    "editAction": {
      "chains": [
        {
          "chain": "ButtonActionChain1",
          "parameters": {
            "event": "{{$event}}",
            "originalEvent": "{{$event.detail.originalEvent}}"
          }
        }
      ]
    },
    "searchAction": {
      "chains": [
        {
          "chain": "SearchButtonActionChain",
          "parameters": {
            "event": "{{$event}}",
            "originalEvent": "{{$event.detail.originalEvent}}"
          }
        }
      ]
    },
    "selectValueItem": {
      "chains": [
        {
          "chain": "SelectValueItemChangeChain",
          "parameters": {
            "event": "{{$event}}",
            "previousValue": "{{$event.detail.previousValue}}",
            "value": "{{$event.detail.value}}",
            "updatedFrom": "{{$event.detail.updatedFrom}}",
            "key": "{{$event.detail.value.key}}",
            "data": "{{$event.detail.value.data}}",
            "metadata": "{{$event.detail.value.metadata}}",
            "valueItem": "{{ $event.detail.value }}"
          }
        }
      ]
    },
    "selectValueItem2": {
      "chains": [
        {
          "chain": "SelectValueItemChangeChain1",
          "parameters": {
            "event": "{{$event}}",
            "previousValue": "{{$event.detail.previousValue}}",
            "value": "{{$event.detail.value}}",
            "updatedFrom": "{{$event.detail.updatedFrom}}",
            "key": "{{$event.detail.value.key}}",
            "data": "{{$event.detail.value.data}}",
            "metadata": "{{$event.detail.value.metadata}}",
            "valueItem": "{{ $event.detail.value }}"
          }
        }
      ]
    },
    "spanClick": {
      "chains": [
        {
          "chain": "spanClickChain",
          "parameters": {
            "event": "{{$event}}",
            "key": "{{ $current.key }}",
            "index": "{{ $current.index }}",
            "current": "{{ $current }}"
          }
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "chain": "PageVbEnterChain",
          "parameters": {
            "event": "{{$event}}"
          }
        }
      ]
    }

  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-dialog": {
        "path": "ojs/ojdialog"
      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-select-single": {
        "path": "ojs/ojselectsingle"
      },
      "oj-sp-welcome-page": {
        "path": "oj-sp/welcome-page/loader"
      },
      "oj-table": {
        "path": "ojs/ojtable"
      }

   }
  }
}