      "defaultValue": [
        {
          "field": "name",
          "headerText": "",
          "sortable": "disabled",
          "width": 200
        },
        {
          "field": "timetype",
          "headerTemplate": "blankHeader",
          "sortable": "disabled",
          "width": 200
        },
        {
          "field": "jobCode",
          "headerTemplate": "blankHeader",
          "width": 200
        },
        {
          "field": "A",
          "headerText": "A",
          "template": "comboTemplate"
        },
        {
          "field": "B",
          "headerText": "B",
          "template": "comboTemplate"
        },
        {
          "field": "C",
          "headerText": "C",
          "template": "comboTemplate"
        },
        {
          "field": "D",
          "headerText": "D",
          "template": "comboTemplate"
        },
        {
          "field": "E",
          "headerText": "E",
          "template": "comboTemplate"
        },
        {
          "field": "F",
          "headerText": "F",
          "template": "comboTemplate"
        }
      ]
    },
    "combinationRows": {
      "type": "object[]"
    },
    "data": {
      "type": "any"
    },
    "dataColumnNames": {
      "type": "string[]",
      "defaultValue": [
        "firstName",
        "lastName",
        "jobObject",
        "salary",
        "jobObject",
        "review"
      ]
    },
    "getCrewRiteProjectSearch2ListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "CrewRite_ORDS/getCrewRite_ProjectSearch2",
        "keyAttributes": "project_name",
        "itemsPath": "items",
        "responseType": "getCrewRiteProjectSearch2Response",
        "transformsContext": {
          "vb-textFilterAttributes": [
            "project_name"
          ]
        }
      }
    },
    "getGetCustomerDetailsListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "TimeRite_Ords_Service/getGetCustomerDetails",
        "keyAttributes": "crew_name",
        "itemsPath": "items",
        "responseType": "getGetCustomerDetailsResponse",
        "transformsContext": {
          "vb-textFilterAttributes": [
            "crew_name"
          ]
        }
      }
    },
    "headerobj": {
      "type": {
        "crew_setupid": "string",
        "crewDate": "string",
        "crewname": "string"
      }
    },
    "illustrationBackground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-bg-01.png"
    },
    "illustrationForeground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-fg-01.png"
    },
    "imageStretch": {
      "type": "string",
      "defaultValue": "none"
    },
    "mobileImage": {
      "type": "string",
      "defaultValue": "on"
    },
    "projectADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "itemType": "getCrewRite_ProjectSearch2",
        "keyAttributes": "project_name"
      }
    },
    "taskADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "itemType": "getProjectIdChildTasks",
        "keyAttributes": "TaskName"
      }
    },
    "taskDP": {
      "type": "object"
    },
    "timeDP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "itemType": "dynamic",
        "keyAttributes": "id"
      }
    },
    "timeEntry": {
      "type": "dynamic"
    },
    "timeGridDP": {
      "type": "any"
    }

  },
  "metadata": {},
  "types": {
    "@decorators.getCrewRite_ProjectSearch2": {
      "endpoint": "getCrewRite_ProjectSearch2",
      "service": "CrewRite_ORDS",
      "path": "response-_-items[0]"
    },
    "@decorators.getCrewRiteProjectSearch2Response": {
      "endpoint": "getCrewRite_ProjectSearch2",
      "service": "CrewRite_ORDS",
      "path": "response"
    },
    "@decorators.getGetCustomerDetailsResponse": {
      "endpoint": "getGetCustomerDetails",
      "service": "TimeRite_Ords_Service",
      "path": "response"
    },
    "@decorators.getProjectIdChildTasks": {
      "endpoint": "getProjectIdChildTasks",
      "service": "FSCM_REST_API",
      "path": "response-_-items[0]"
    },
    "dynamic": {
      "A": "any",
      "B": "any",
      "C": "any",
      "D": "any",
      "E": "any",
      "F": "any",
      "id": "number",
      "jobCode": "string",
      "name": "string",
      "Project_A": "string",
      "timetype": "string"

    },
    "getCrewRite_ProjectSearch2": {
      "project_id": "number",
      "project_name": "string",
      "project_number": "string"
    },
    "getCrewRiteProjectSearch2Response": {
      "items": [
        {
          "project_id": "number",
          "project_name": "string",
          "project_number": "string"
        }
      ]
    },
    "getGetCustomerDetailsResponse": {
      "items": [
        {
          "crew_name": "string",
          "crewsetup_id": "number"
        }
      ]
    },
    "getProjectIdChildTasks": {
      "TaskFinishDate": "string",
      "TaskId": "number",
      "TaskName": "string",
      "TaskNumber": "string"
    }

  },
  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chain": "AddColumn",
          "parameters": {
            "event": "{{$event}}",
            "originalEvent": "{{$event.detail.originalEvent}}"
          }
        }
      ]
    },
    "buttonAction2": {
      "chains": [
        {
          "chain": "SearchButtonActionChain",
          "parameters": {
            "event": "{{$event}}",
            "originalEvent": "{{$event.detail.originalEvent}}"
          }
        }
      ]
    },
    "buttonAction3": {
      "chains": [
        {
          "chain": "SaveButtonActionChain",
          "parameters": {
            "event": "{{$event}}",
            "originalEvent": "{{$event.detail.originalEvent}}"
          }
        }
      ]
    },
    "selectValueItem": {
      "chains": [
        {
          "chain": "SelectValueItemChangeChain",
          "parameters": {
            "event": "{{$event}}",
            "previousValue": "{{$event.detail.previousValue}}",
            "value": "{{$event.detail.value}}",
            "updatedFrom": "{{$event.detail.updatedFrom}}",
            "key": "{{$event.detail.value.key}}",
            "data": "{{$event.detail.value.data}}",
            "metadata": "{{$event.detail.value.metadata}}",
            "valueItem": "{{ $event.detail.value }}",
            "current": "[[ $current ]]"
          }
        }
      ]
    },
    "selectValueItem2": {
      "chains": [
        {
          "chain": "CrewNameSelectValueItemChangeChain",
          "parameters": {
            "event": "{{$event}}",
            "previousValue": "{{$event.detail.previousValue}}",
            "value": "{{$event.detail.value}}",
            "updatedFrom": "{{$event.detail.updatedFrom}}",
            "key": "{{$event.detail.value.key}}",
            "data": "{{$event.detail.value.data}}",
            "metadata": "{{$event.detail.value.metadata}}",
            "valueItem": "{{ $event.detail.value }}"
          }
        }
      ]
    },
    "selectValueItem3": {
      "chains": [
        {
          "chain": "SelectValueItemChangeChain1",
          "parameters": {
            "event": "{{$event}}",
            "previousValue": "{{$event.detail.previousValue}}",
            "value": "{{$event.detail.value}}",
            "updatedFrom": "{{$event.detail.updatedFrom}}",
            "key": "{{ $current.key }}",
            "data": "{{$event.detail.value.data}}",
            "metadata": "{{$event.detail.value.metadata}}",
            "valueItem": "{{ $event.detail.value }}",
            "index": "{{ $current.index }}",
            "current": "{{ $current }}"
          }
        }
      ]
    },
    "timeTableBeforeRowEdit": {
      "chains": [
        {
          "chain": "TableBeforeRowEditChain",
          "parameters": {
            "event": "{{$event}}",
            "accept": "{{$event.detail.accept}}",
            "rowContext": "{{$event.detail.rowContext}}",
            "rowKey": "{{$event.detail.rowContext.status.rowKey}}",
            "rowIndex": "{{$event.detail.rowContext.status.rowIndex}}",
            "rowData": "{{$event.detail.rowContext.componentElement.getDataForVisibleRow($event.detail.rowContext.status.rowIndex).data}}"
          }
        }
      ]
    },
    "timeTableBeforeRowEditEnd": {
      "chains": [
        {
          "chain": "TableBeforeRowEditEndChain",
          "parameters": {
            "event": "{{$event}}",
            "accept": "{{$event.detail.accept}}",
            "setUpdatedItem": "{{$event.detail.setUpdatedItem}}",
            "rowContext": "{{$event.detail.rowContext}}",
            "cancelEdit": "{{$event.detail.cancelEdit}}",
            "rowKey": "{{$event.detail.rowContext.status.rowKey}}",
            "rowIndex": "{{$event.detail.rowContext.status.rowIndex}}",
            "rowData": "{{$event.detail.rowContext.componentElement.getDataForVisibleRow($event.detail.rowContext.status.rowIndex).data}}"
          }
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "chain": "PageVbEnterChain",
          "parameters": {
            "event": "{{$event}}"
          }
        }
      ]
    }

